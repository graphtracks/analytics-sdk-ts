{
	"name": "@graphtracks/client",
	"version": "1.0.0",
	"description": "GraphTracks API Bluesky Analytics client for TypeScript",
	"author": "GraphTracks",
	"repository": {
		"type": "git",
		"url": "https://github.com/graphtracks/analytics-sdk-ts.git"
	},
	"keywords": ["bluesky", "analytics", "api", "atproto", "graphtracks"],
	"license": "Apache-2.0",
	"main": "./dist/index.js",
	"typings": "./dist/index.d.ts",
	"module": "./dist/esm/index.js",
	"sideEffects": false,
	"scripts": {
		"build": "tsc && tsc -p tsconfig.esm.json",
		"prepare": "npm run build",
		"format": "biome check --write .",
		"lint": "biome check .",
		"test": "jest"
	},
	"dependencies": {
		"axios": "^1.6.1"
	},
	"jest": {
		"testMatch": ["**/tests/**/*.test.ts"],
		"transform": {
			"^.+\\.ts$": "ts-jest"
		},
		"silent": false
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "^22.15.17",
		"ts-node": "^10.9.2",
		"typescript": "^5.5.4",
		"jest": "^29.7.0",
		"ts-jest": "^29.3.4",
		"@types/jest": "^29.5.14"
	}
}
