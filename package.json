{
  "name": "@graphtracks/client",
  "version": "1.2.2",
  "description": "GraphTracks API Bluesky Analytics client for TypeScript",
  "author": "GraphTracks",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/graphtracks/analytics-sdk-ts.git"
  },
  "keywords": [
    "bluesky",
    "analytics",
    "api",
    "atproto",
    "graphtracks"
  ],
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "module": "./dist/esm/index.js",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json",
    "package-lock.json",
    "tsconfig.json",
    "tsconfig.esm.json",
    "tsconfig.lib.json",
    "CHANGELOG.md",
    "*.ts"
  ],
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "prepare": "npm run build",
    "format": "biome check --write .",
    "lint": "biome check .",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^1.10.0"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "silent": false,
    "testTimeout": 100000
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.15.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "@types/jest": "^29.5.14"
  }
}
